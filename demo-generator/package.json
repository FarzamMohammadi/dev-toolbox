{
  "name": "demo-generator",
  "version": "1.0.0",
  "description": "Generate animated web-based demo videos with Claude",
  "type": "module",
  "scripts": {
    "dev": "npx serve . -p 3000 --no-clipboard",
    "check": "bash setup/check-dependencies.sh",
    "beats": "uv run python scripts/analyze-beats.py",
    "clean": "rm -f output/*.html output/*.md output/*.json output/*.mp4",
    "record": "node scripts/record-scenes.js",
    "assemble": "node scripts/assemble.js"
  },
  "devDependencies": {
    "puppeteer": "^24.0.0",
    "serve": "^14.2.0"
  }
}
